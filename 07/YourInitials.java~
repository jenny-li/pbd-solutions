public class YourInitials
{
	public static void main( String[] args )
	{
	System.out.println("JJJJJ  L");
	System.out.println("  J    L");
	System.out.println("  J    L");
	System.out.println("  J    L");
	System.out.println("J J    L");
	System.out.println("J J    L");
	System.out.println(" JJ    LLLLL");

	}
}
